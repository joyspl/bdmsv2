@model IEnumerable<SARASWATIPRESSNEW.Models.MstDistrictDtl>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_BdmsLayoutPage.cshtml";
}
<script>
    function ExportToExcel() {
        var url = "/MstDistrict/ExportDistrictData";
        window.location.href = url;
    }
</script>
<div class="panel-heading">
    <h3 class="panel-title"><strong>District Master</strong></h3>
</div>

<div class="panel-body">
    <table id="tblBtn" cellpadding="0" class="table" cellspacing="0" border="0" style="width: 100%;">
        <tr>
            <td><a href="/MstDistrict/AddNewDistrict" style="width: 120px;" class="btn btn-info btn-block">Add New</a></td>
            <td>
                <input type="button" id="btnEmail" value="Export To Excel" onclick="ExportToExcel();" style="width: 120px" class="btn btn-info btn-block" /></td>
        </tr>
    </table>
    <div style="width: 100%; height: 450px; overflow: scroll;">
        <table border="0" style="width: 400px;" class="table">
            <tr>
                <th style="text-align: left; width: 20%;">District Name</th>
            </tr>
            @try
            {
                foreach (var objModel in Model)
                {
                <tr>
                    <td style="text-align: left">
                        <input id="hdId" value="@objModel.districtId" type="hidden" />
                        @objModel.districtName
                    </td>
                </tr>
                }
            }
            catch (Exception ex)
            {
                <tr>
                    <td style="text-align: center"></td>
                </tr>
            }
        </table>
    </div>
</div>
