@model IEnumerable<SARASWATIPRESSNEW.Models.Book>
@{
    ViewBag.Title = "Index";
}

<h2>Book List</h2>

    <table width="100%">
        <tr>
            <th>Book Code</th> 
            <th>Language</th>
            <th>Category</th>
            <th>Book Name</th>
            <th>Rate</th>
            <th>Quantity</th>
            <th>Price</th>           
        </tr>
        @foreach(var req in Model)
        {
        <tr>
            <td style="text-align:center">@req.BookCode</td> 
            <td style="text-align:center">@req.LanguageName</td>
            <td style="text-align:center">@req.CategoryName</td>
            <td style="text-align:center">@req.BookName</td>
            <td style="text-align:center">@req.rate</td>
            <td style="text-align:center">@req.quantity</td>
            <td style="text-align:center">@req.unitprice</td>          
        </tr>
        }
    </table>
