@model IEnumerable<SARASWATIPRESSNEW.Models.Circle>
@{
    ViewBag.Title = "Index";
}

<h1>Circle List</h1>
    <table width="100%">
        <tr>
            <th>District</th>
            <th>Circle Code</th>
            <th>Circle Name</th>            
        </tr>
        @foreach(var req in Model)
        {
        <tr>
            <td style="text-align:center">@req.district_id</td>
            <td style="text-align:center">@req.Circle_code</td>
            <td style="text-align:center">@req.Circle_name</td>           
        </tr>
        }
    </table>
