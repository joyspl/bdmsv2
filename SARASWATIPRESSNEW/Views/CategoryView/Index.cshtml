@model IEnumerable<SARASWATIPRESSNEW.Models.Category>
@{
    ViewBag.Title = "Index";
    Layout = "";
}

<h1>Category List</h1>
    <table width="100%">
        <tr>
            <th>Category</th>            
        </tr>
        @foreach(var req in Model)
        {
        <tr>
            <td style="text-align:center">@req.Category_name</td>           
        </tr>
        }
    </table>